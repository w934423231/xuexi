.userlogn{
    width: 100%;
    height:320rpx;
    display: flex;
    justify-content: center;
    background-color:rgba(233, 233, 233, 0.966);
    align-items: center;
}
.userlogn>view{
    width:300rpx;
    height:300rpx;
    display: flex;
    flex-direction:column;
    justify-content: center;
    align-items: center
}
.userlogn image{
    width:240rpx;
    height:250px;
    border-radius:50%;
}
.userlogn>view>view{
    color: rgb(92, 91, 91);
    font-size:40rpx;
    margin-top:20rpx;
}
.xinxiguanli{
    width:100%;
    height:115rpx;
    padding:0px 30rpx;
    border-bottom:1px solid gray;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content:space-between;
    align-items: center
}
.xinxiguanli>view:first-child{
    width:300rpx;
    height:80rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    align-items: center
}
.tubiao{
    font-size:60rpx;
    color: orange;
    margin-right:20rpx;
}
.font{
    font-size:35rpx;
}
.fons{
    font-size:35rpx;
    font-weight: bold;
}
.xinxiguanli>view:last-child{
    width:80rpx;
    height:80rpx;
    display: flex;
    align-items: center
}
.userout{
    width:100%;
    height:60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top:30rpx;
}
.userout view{
   font-size:35rpx 
}